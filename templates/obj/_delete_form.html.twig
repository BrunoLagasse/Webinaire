<form method="post" action="{{ path('obj_delete', {'id': obj.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer le webinaire?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ obj.id) }}">
    <button class="btn btn-danger m-1">Supprimer</button>
</form>
