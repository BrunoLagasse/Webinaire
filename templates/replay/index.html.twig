    <body>       
    <style>
        iframe {
            width:  100% !important;
            height: 100% !important;
            position: relative;
            z-index: 5;
        }

        body{
            background : -webkit-linear-gradient( 0deg, #8702DB 5%, #4B60E0 40.44%, #00D4E7 79.09% );
            height:100vh;
            width:100vw;
            z-index: -1;
        }

        body::before{
            background: white;
            content: "";
            opacity:1;
            top: 37vh;
            left: -5vw;
            bottom: 0;
            right: 0;
            height:90vh;
            width:110vw;
            transform:rotate(-5deg);
            position: fixed;
            z-index: 0;
        }

        body::after{
            content: "";
            width:45vw;
            height:45vw;
            border-radius:50%;
            position: fixed;
            background: #FFC700;
            top: 15vh;
            left: -13vw;
            filter: blur(48.5342px);
            transform: matrix(-0.71, -0.71, 0.71, -0.71, 0, 0);
            z-index: -1;
        }
        div#first{
            width:50vw;
            background: white;
            padding:1em;
            margin:5em auto;
            font-family:sans-serif;
            z-index: 1;
            position: relative;
        }
        div#first div{
            margin-top:1em;
        }
    </style>
